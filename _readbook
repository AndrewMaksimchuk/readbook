#compdef readbook
# Add competion for shell(zsh)


local -a subcmds
subcmds=(
    'add:add new book' 
    'read:read next page, or page you put as argument' 
    'rn:shorthand for read command' 
    'current:read current page' 
    'rc:shorthand for "current" command' 
    'book:about book' 
    'open:open book in default application' 
)
_describe 'command' subcmds
